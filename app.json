{
  "expo": {
    "name": "Manga App",
    "slug": "mangaApp",
    "version": "1.0.0",
    "privacy":"public",
    "description":"app for reading manga bich",
    "orientation": "portrait",
    "icon": "./assets/takezosquare.png",
    "splash": {
      "image": "./assets/vaga.png",
      "resizeMode": "cover",
      "backgroundColor": "#B6D4DB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/takezosquare.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "manga.app.beta"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
